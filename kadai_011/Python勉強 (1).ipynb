        "#forを使う\n",
        "array = [\"水\", \"金\", \"地\", \"火\", \"木\", \"土\", \"天\", \"海\", \"冥\"]\n",
        "\n",
        "for i in range(1):\n",
        "    for value in array:\n",
        "        print(value)\n",
        "\n",
        "#whileを使う\n",
        "\n",
        "num = 0\n",
        "while num < 9:\n",
        "    print(array[num])\n",
        "    num = num + 1"
